    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiceRedirect</servlet-name>
        <url-pattern>/ServiceRedirect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddService</servlet-name>
        <url-pattern>/AddService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveService</servlet-name>
        <url-pattern>/RemoveService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.jsp</welcome-file>
    </welcome-file-list>
    <jsp-config>
        <jsp-property-group>
            <display-name>Admin</display-name>
            <url-pattern>/administration/Login/index.jsp</url-pattern>
        </jsp-property-group>
    </jsp-config>
</web-app>
