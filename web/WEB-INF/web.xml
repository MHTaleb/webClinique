        <url-pattern>/UserConnect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consultation</servlet-name>
        <url-pattern>/Consultation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Medicaments</servlet-name>
        <url-pattern>/Medicaments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicamentServlet</servlet-name>
        <url-pattern>/MedicamentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMedicament</servlet-name>
        <url-pattern>/AddMedicament</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InvestigationServlet</servlet-name>
        <url-pattern>/InvestigationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddInvestigation</servlet-name>
        <url-pattern>/AddInvestigation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutAdmin</servlet-name>
        <url-pattern>/LogoutAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminPage</servlet-name>
        <url-pattern>/AdminPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMedecin</servlet-name>
        <url-pattern>/AddMedecin</url-pattern>
